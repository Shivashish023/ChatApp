{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/send/(.*)",
        "methods": ["POST"],
        "dest": "server.js"
      },
      {
        "src": "/api/(.*)",
        "methods": ["GET"],
        "dest": "server.js"
      },
      {
        "src": "/api/register",
        "methods": ["POST"],
        "dest": "server.js"
      },
      {
        "src": "/api/login",
        "methods": ["POST"],
        "dest": "server.js"
      },
      {
        "src": "/api/logout",
        "methods": ["GET"],
        "dest": "server.js"
      },
      {
        "src": "/api/users",
        "methods": ["GET"],
        "dest": "server.js"
      }
    ]
}